<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    tools:context="com.szy.demos.ui.activity.time.TimeThridActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_thrid_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="日期选择器" />

        <Button
            android:id="@+id/btn_thrid_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="时间选择器" />

        <Button
            android:id="@+id/btn_thrid_14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="日期时间选择器" />

        <Button
            android:id="@+id/btn_thrid_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="单项选择器" />

        <Button
            android:id="@+id/btn_thrid_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="数字选择器" />

        <Button
            android:id="@+id/btn_thrid_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="地址选择器（含省级、地级、县级）" />

        <Button
            android:id="@+id/btn_thrid_6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="地址选择器（含地级、县级）" />

        <Button
            android:id="@+id/btn_thrid_7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="车牌号选择器" />

        <Button
            android:id="@+id/btn_thrid_8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="二级选择" />

        <Button
            android:id="@+id/btn_thrid_9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="单项选择器" />

        <Button
            android:id="@+id/btn_thrid_10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="选择时间" />

        <Button
            android:id="@+id/btn_thrid_11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="目录选择器"
            android:visibility="gone" />

        <Button
            android:id="@+id/btn_thrid_12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="颜色选择器" />

        <Button
            android:id="@+id/btn_thrid_13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="目录选择器" />

        <Button
            android:id="@+id/btn_thrid_15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="自定义选择器" />
    </LinearLayout>
</ScrollView>
